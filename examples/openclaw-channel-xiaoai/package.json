{
  "name": "@openclaw/xiaoai",
  "version": "1.0.0",
  "description": "小爱音箱接入OpenClaw",
  "private": true,
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "build": "cd ../migpt && cargo-cp-artifact -ac open_xiaoai_server ../openclaw-channel-xiaoai/src/open-xiaoai.node -- cargo build --release --message-format=json-render-diagnostics",
    "start": "tsx index.ts",
    "dev": "pnpm build && pnpm start"
  },
  "dependencies": {
    "@mi-gpt/engine": "1.2.1",
    "@mi-gpt/utils": "1.1.1"
  },
  "devDependencies": {
    "@neon-rs/cli": "0.1.82",
    "@types/node": "^22.13.11",
    "cargo-cp-artifact": "^0.1.9",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "packageManager": "pnpm@9.15.9",
  "engines": {
    "node": ">=16"
  },
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "channels": [
      "xiaoai"
    ],
    "installDependencies": true,
    "channel": {
      "id": "xiaoai",
      "label": "XiaoAi Speaker",
      "selectionLabel": "XiaoAi音箱",
      "docsPath": "/channels/xiaoai",
      "docsLabel": "xiaoai",
      "blurb": "小爱音箱接入OpenClaw",
      "order": 71,
    },
    "install": {
      "npmSpec": "@openclaw/xiaoai",
      "localPath": "extensions/xiaoai-channel",
      "defaultChoice": "local"
    }
  }
}
